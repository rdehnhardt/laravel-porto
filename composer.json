{
    "name": "rdehnhardt/laravel-porto",
    "license": "MIT",
    "authors": [
        {
            "name": "Renato Dehnhardt",
            "email": "renatotkd@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~6.0",
        "apiato/core": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Porto\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Porto\\Providers\\PortoServiceProvider",
                "Illuminate\\Foundation\\Support\\Providers\\RouteServiceProvider"
            ],
            "aliases": {
                "Porto": "Porto\\Foundation\\Facades\\Porto"
            }
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "apiato/core",
                "version": "2.4.8",
                "source": {
                    "url": "git@github.com:fullsystem/core.git",
                    "type": "vcs",
                    "reference": "branches/master"
                }
            }
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fullsystem/heimdal.git"
        }
    ]
}
