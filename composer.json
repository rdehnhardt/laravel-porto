{
    "name": "rdehnhardt/laravel-porto",
    "license": "MIT",
    "authors": [
        {
            "name": "Renato Dehnhardt",
            "email": "renatotkd@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~6.0",
        "fullsystem/core": "~2.4",
        "fullsystem/heimdal": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Porto\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Porto\\Providers\\PortoServiceProvider",
                "Illuminate\\Foundation\\Support\\Providers\\RouteServiceProvider"
            ],
            "aliases": {
                "Porto": "Porto\\Foundation\\Facades\\Porto"
            }
        }
    },
    "repositories": [
        { "name": "fullsystem/core", "type": "vcs", "url": "https://github.com/fullsystem/core" },
        { "name": "fullsystem/heimdal", "type": "vcs", "url": "http://svn.example.org/private/repo" }
    ]
}
